{
    "name": "rebelcode/rcmod-eddbk-services",
    "description": "A RebelCode module that adds EDD Downloads as booking services, as used in EDD Bookings.",
    "type": "rc-module",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "rebelcode/modular": "dev-task/initial-classes",
        "rebelcode/transformers": "dev-task/initial-classes",
        "dhii/cqrs-resource-model-interface": "^0.1-alpha2",
        "dhii/iterator-helper-base": "^0.1-alpha2",
        "dhii/memoize-memory": "^0.2-alpha1",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\EddBookings\\Services\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    }
}
